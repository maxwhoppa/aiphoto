{
  "cli": {
    "version": ">= 5.0.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "resourceClass": "m-medium",
        "simulator": true
      },
      "android": {
        "buildType": "apk"
      }
    },
    "preview": {
      "distribution": "internal",
      "ios": {
        "resourceClass": "m-medium",
        "entitlements": {
          "com.apple.developer.applesignin": ["Default"]
        }
      }
    },
    "production": {
      "ios": {
        "resourceClass": "m-medium",
        "autoIncrement": true,
        "entitlements": {
          "com.apple.developer.applesignin": ["Default"]
        }
      }
    }
  },
  "submit": {
    "production": {
      "ios": {
        "appleId": "maxwellsimonnewman@gmail.com",
        "ascAppId": "6753353761",
        "appleTeamId": "2QGZ85CE4S"
      }
    }
  }
}
