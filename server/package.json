{
  "name": "aiphoto-server",
  "version": "1.0.0",
  "description": "AI Photo Processing Server",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index-simple.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "db:generate": "drizzle-kit generate:pg",
    "db:migrate": "tsx src/db/migrate.ts",
    "db:studio": "drizzle-kit studio",
    "deploy": "sst deploy",
    "deploy:staging": "sst deploy --stage staging",
    "deploy:prod": "sst deploy --stage prod",
    "remove:dev": "sst remove",
    "remove:staging": "sst remove --stage staging",
    "remove:prod": "sst remove --stage prod"
  },
  "dependencies": {
    "@aws-lambda-powertools/logger": "^2.0.0",
    "@aws-lambda-powertools/metrics": "^2.0.0",
    "@aws-lambda-powertools/tracer": "^2.0.0",
    "@aws-sdk/client-s3": "^3.511.0",
    "@aws-sdk/client-sqs": "^3.511.0",
    "@aws-sdk/client-ssm": "^3.511.0",
    "@aws-sdk/s3-request-presigner": "^3.511.0",
    "@fastify/cors": "^8.4.2",
    "@fastify/helmet": "^11.1.1",
    "@fastify/multipart": "^8.1.0",
    "@fastify/rate-limit": "^9.1.0",
    "@google/generative-ai": "^0.15.0",
    "@sentry/node": "^7.99.0",
    "@trpc/server": "^10.45.2",
    "aws-cdk-lib": "^2.215.0",
    "constructs": "^10.4.2",
    "drizzle-orm": "^0.29.3",
    "fastify": "^4.25.2",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "jwks-client": "^2.0.5",
    "mime-types": "^2.1.35",
    "postgres": "^3.4.3",
    "sharp": "^0.33.2",
    "uuid": "^9.0.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.152",
    "@types/jest": "^29.5.11",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/mime-types": "^2.1.4",
    "@types/node": "^20.11.16",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "drizzle-kit": "^0.20.14",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "sst": "^2.41.4",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
