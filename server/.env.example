# Database
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/aiphoto

# Redis (optional)
REDIS_URL=redis://localhost:6379

# AWS Configuration
AWS_REGION=us-east-1
AWS_ACCESS_KEY_ID=your_access_key_id
AWS_SECRET_ACCESS_KEY=your_secret_access_key
S3_BUCKET_NAME=your_bucket_name
SQS_QUEUE_URL=https://sqs.us-east-1.amazonaws.com/account/queue-name

# Cognito
COGNITO_USER_POOL_ID=your_user_pool_id
COGNITO_REGION=us-east-1

# Google Gemini
GOOGLE_GEMINI_API_KEY=your_gemini_api_key

# Application
NODE_ENV=development
PORT=3000
CORS_ORIGIN=http://localhost:3000,http://localhost:19006,http://localhost:8081

# Monitoring (optional)
SENTRY_DSN=your_sentry_dsn

STRIPE_SECRET_KEY=ex
STRIPE_WEBHOOK_SECRET=ex